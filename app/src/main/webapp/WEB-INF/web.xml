<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>OTD</display-name>
  <filter>
  	<filter-name>CorsFilter</filter-name>
  	<filter-class>br.com.cors.CorsFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>CorsFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>CorsServlet</servlet-name>
  	<display-name>CorsServlet</display-name>
  	<description></description>
  	<servlet-class>br.com.cors.CorsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CorsServlet</servlet-name>
  	<url-pattern>/*</url-pattern>
  </servlet-mapping>

</web-app>
